<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.timeOrganizer.view.StartController">
    <children>
        <SplitPane dividerPositions="0.5" prefHeight="160.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
                   AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <Button fx:id="UserLogInButton" layoutX="88.0" layoutY="113.0" mnemonicParsing="false"
                                onMouseClicked="#UserLogInButtonClicked" text="User log in">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Button>
                        <TextField fx:id="UserLogInTextField" layoutX="108.0" layoutY="260.0" prefHeight="37.0"
                                   prefWidth="159.0"/>
                        <Label fx:id="UserLogInLabel" layoutX="40.0" layoutY="232.0" prefHeight="20.0" prefWidth="99.0"
                               text="User Name">
                            <font>
                                <Font size="14.0"/>
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                    <children>
                        <Button fx:id="OrganizerLogInButton" layoutX="84.0" layoutY="174.0" mnemonicParsing="false"
                                prefHeight="25.0" prefWidth="129.0" text="Organizer log in">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Button>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>
