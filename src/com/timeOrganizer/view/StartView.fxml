<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@DarkTheme.css"
            xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.timeOrganizer.view.StartViewController">
    <children>
        <VBox layoutY="79.0" prefHeight="321.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
              AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0" AnchorPane.topAnchor="79.0">
            <children>
                <Button fx:id="friendsButton" mnemonicParsing="false" onAction="#friendsButtonClicked" prefWidth="200.0"
                        stylesheets="@DarkTheme.css" text="Friends">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="adventuresButton" mnemonicParsing="false" onAction="#adventuresButtonClicked"
                        prefWidth="200.0" stylesheets="@DarkTheme.css" text="Adventures">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="eventCreatorButton" mnemonicParsing="false" onAction="#adventureCreatorButtonClicked"
                        prefHeight="33.0" prefWidth="265.0" text="Event creator">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#invitationsButtonClicked" prefHeight="33.0" prefWidth="293.0"
                        text="Invitations">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
                <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#logOutButtonClicked" prefHeight="25.0"
                        prefWidth="269.0" stylesheets="@DarkTheme.css" text="Log out">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
